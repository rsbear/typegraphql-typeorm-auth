{
  "name": "typegraphql-typeorm-auth",
  "version": "1.0.0",
  "description": "GraphQL API and auth system using refresh tokens",
  "main": "index.js",
  "author": "rsbear",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "nodemon --exec ts-node src/index.ts",
    "migration:create": "run migration:create -n all",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n all",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.14.0",
    "rword": "^3.1.1",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.20",
    "uniqid": "^5.2.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.14",
    "@types/rword": "^3.0.0",
    "@types/uniqid": "^4.1.3",
    "nodemon": "^2.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  }
}
